{
  "compilerOptions": {
    "target": "es5",
    "lib": [
        "es6",
        "dom",
        "dom.iterable",
        "esnext"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/elements/Markdown.tsx", "src/components/elements/NavLink.js"  ],
  "exclude": [
    "node_modules"
  ]
}
